 <form action="/teams/create" method="post">
          <div class="row">
           <div class="col-md-6 col-md-offset-3">
              <label>Name</label>
              <input type="text" name="name" style="display: block; width: 100%; height: 34px;padding: 6px 12px; font-size: 14px; border-radius: 4px;" class="form-control"></input>
              <label>Location</label>
              <input type="text" name="location" style="display: block; width: 100%; height: 34px;padding: 6px 12px; font-size: 14px; border-radius: 4px;"  class="form-control"></input>
              <label>Created Date</label>
              <input type="date" name="created_date" style="display: block; width: 100%; height: 34px;padding: 6px 12px; font-size: 14px; border-radius: 4px;"  class="form-control"></input>
              <input type="submit" name="submit" id="submit" value="Add Team" class="btn btn-primary" />
            </div>
          </div>
</form>
