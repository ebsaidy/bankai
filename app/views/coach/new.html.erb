 <form action="/coach/create" method="post">
          <div class="row">
           <div class="col-md-6 col-md-offset-3">
              <label>First Name</label>
              <input type="text" name="first_name" style="display: block; width: 100%; height: 34px;padding: 6px 12px; font-size: 14px; border-radius: 4px;" class="form-control"></input>
              <label>Middle Name</label>
              <input type="text" name="middle_name" style="display: block; width: 100%; height: 34px;padding: 6px 12px; font-size: 14px; border-radius: 4px;"  class="form-control"></input>
              <label>Last Name</label>
              <input type="text" name="last_name" style="display: block; width: 100%; height: 34px;padding: 6px 12px; font-size: 14px; border-radius: 4px;"  class="form-control"></input>
              <label>Date Of Birth</label>
              <input type="date" name="date_of_birth" style="display: block; width: 100%; height: 34px;padding: 6px 12px; font-size: 14px; border-radius: 4px;"  class="form-control"></input>
              <label>Place Of Birth</label>
              <input type="text" name="place_of_birth" style="display: block; width: 100%; height: 34px;padding: 6px 12px; font-size: 14px; border-radius: 4px;"  class="form-control"></input>
              <label>Address</label>
              <input type="text" name="address" style="display: block; width: 100%; height: 34px;padding: 6px 12px; font-size: 14px; border-radius: 4px;"  class="form-control"></input>
              <input type="submit" name="submit" id="submit" value="Add Coach" class="btn btn-primary" />
            </div>
          </div>
</form>

